    <featureTypes>
        <atom:link xmlns:atom="http://www.w3.org/2005/Atom" rel="alternate" href="http://localhost:8080/geoserver/rest/workspaces/demapp/datastores/demapp/featuretypes.xml" type="application/xml"/>
